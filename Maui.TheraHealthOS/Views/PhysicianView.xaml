<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Maui.TheraHealthOS.Views.PhysicianView"
             NavigatedTo= "ContentPage_NavigatedTo">
<VerticalStackLayout>
    <Label Text="Full Name"/>
    <Entry Text="{Binding Name}"/>
    <Label Text="License Number"/>
    <Entry Text="{Binding LicenseNumber}"/>
    <Label Text="Graduation Date"/>
    <DatePicker Date="{Binding GraduationDate}"/>  
    <Label Text="Specializations (comma separated)"/>
    <Entry Text="{Binding Specializations}"/>

      <Button Text="Cancel" Clicked="CancelPhysicianClicked"/>
      <Button Text="Ok" Clicked="OkPhysicianClicked"/>
</VerticalStackLayout>
</ContentPage>